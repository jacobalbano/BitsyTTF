{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
		"eslint:recommended",
		"plugin:jsdoc/recommended"
	],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
	
	"plugins": [
		"jsdoc"
	],
    "rules": {
		"semi": "error",
		"eqeqeq": ["error", "smart"]
    }
}
